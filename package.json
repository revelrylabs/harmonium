{
  "name": "awesome-possum",
  "version": "3.2.1",
  "description": "An opinionated React component framework for teams that move fast.",
  "repository": {
    "type": "git",
    "url": "https://github.com/revelrylabs/possum.git"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "backbone": "^1.3.3",
    "body-parser": "^1.17.2",
    "bowser": "^1.7.0",
    "breakpoint-sass": "^2.7.1",
    "browserify": "^14.4.0",
    "classnames": "^2.2.5",
    "express": "^4.15.3",
    "foundation-sites": "^6.3.1",
    "jquery": "^3.2.1",
    "luxon": "^0.3.1",
    "node-dir": "^0.1.17",
    "node-sass": "^4.5.3",
    "node-static": "^0.7.9",
    "prop-types": "^15.5.7",
    "react": ">=15.3.0",
    "react-dom": ">=15.3.0",
    "sassy-npm-importer": "^2.1.0",
    "source-map-support": "^0.4.15",
    "static-server": "^2.0.4",
    "tough-cookie": "^2.3.2",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4"
  },
  "scripts": {
    "clean": "rm -rf lib/",
    "babel": "babel --source-maps -d lib/ src/",
    "build": "npm run clean && npm run babel && npm run docs",
    "test": "NODE_ENV=test nyc mocha './src/**/*.test.js' --opts ./mocha.opts && nyc report --reporter=html",
    "docs": "node scripts/docs.js",
    "web": "static-server docs -p $PORT",
    "dev": "gulp",
    "postinstall": "npm run clean && npm run babel && node scripts/postinstall.js"
  },
  "keywords": [],
  "author": {
    "name": "Revelry Labs",
    "email": "support@revelry.co",
    "url": "https://revelry.co"
  },
  "contributors": [
    {
      "name": "Joel Wietelmann",
      "email": "joel@revelry.co"
    },
    {
      "name": "Robert Prehn",
      "email": "robert@revelry.co"
    },
    {
      "name": "Laura Hill",
      "email": "laura@revelry.co"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.4",
    "chai": "^3.5.0",
    "enzyme": ">=2.8.2",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-prettier": "^2.5.0",
    "gulp": "^3.9.1",
    "gulp-webserver": "^0.9.1",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.4.2",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": ">=15.0.0",
    "sinon": "^2.3.8"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  }
}
